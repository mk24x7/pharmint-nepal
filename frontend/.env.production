# Production Environment Variables
# Cache Configuration
NEXT_CACHE_HANDLER_PATH=./cache-handler.js
NEXT_CACHE_MAX_MEMORY_SIZE=52428800

# Build optimization
NEXT_BUILD_STATIC_WORKER_COUNT=4
NEXT_OPTIMIZE_FONTS=true
NEXT_OPTIMIZE_IMAGES=true

# Performance settings
NEXT_DISABLE_STATIC_EXPORT_ERRORS=false
NEXT_SHARP=1

# Cache security
CACHE_ENCRYPTION_KEY=production-change-this-key

# Development cache prevention
NODE_ENV=production